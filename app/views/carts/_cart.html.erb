<tr>
	<td><%= c[:name] %></td>
	<td><%= c[:email]%></td>
	<td><%= c[:datefrom] %></td>
	<% if check_status(c[:verify]) == "Pending" %>
		<td>
			<%= link_to "", accept_cart_path(c[:id]) ,class: "fas fa-check fa-2x",style: "color: green" %>
			<%= link_to "", decline_cart_path(c[:id]) ,class: "fa fa-times fa-2x",style: "color: red; padding-left: 30px;"%>
		</td>
	<% else %>
		<% if  check_status(c[:verify]) == "Accept" %>
			<td style="color: green"><%= check_status(c[:verify]) %></td>
		<% elsif check_status(c[:verify]) == "Decline" %>
			<td style="color: red"><%= check_status(c[:verify]) %></td>
		<% end %>
	<% end %>
	<td>
		<%= link_to "", detail_cart_path(c[:id]), class: "fas fa-info fa-2x",style: "color: #035777; padding-left: 17px" %>
	</td>
</tr>
